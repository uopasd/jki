            })
            --
            local UIGradient_32312 = Library:CreateObject("UIGradient", {
                Rotation = 90,
                Color = ColorSequence.new{
                    ColorSequenceKeypoint.new(0, Library.Theme.Default.Accent),
                    ColorSequenceKeypoint.new(1, Library.Theme.Default.SecondAccent)
                },
                Parent = TopBar_5
            })
            --
