            LastCopiedColor = nil,
            WatermarkRefreshRate = 0.15,
            CurrentSelectedColorPicker = nil,
            CurrentSelectedKeybindMode = nil,
            TotalColorPickers = 0,
            TotalKeybindModes = 0,
            WatermarkPosition = "Top Right",
