            DropdownZIndex = 1,
            IndicatorGlow = nil,
            OpenColorFrames = 0,
            HoverHighlight = false,
            WindowGlow = true,
            WindowGlowThickness = 50,
            ScreenGUI = nil,
            Font = Font.new(getcustomasset("LuckyHub/Fonts/Tahoma.font"), Enum.FontWeight.Regular),
