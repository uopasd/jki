	Name = "Melee Aura",
	Default = false,
	Flag = "",
	Callback = function(value)
		_G.MeleeAuraEnabled = value

		if value then
			local AttachTick = tick()
			local AttachCD = {
				["Fists"] = 0.05,
				["BBaton"] = 0.5,
