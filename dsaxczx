                --
                local Name_6 = Library:CreateObject("TextLabel", {
                    FontFace = Library.UI.Font,
                    TextColor3 = Library.Theme.Default.TextColor,
                    BorderColor3 = Color3.fromRGB(0, 0, 0),
                    Text = Options.Name,
                    TextStrokeTransparency = 0,
